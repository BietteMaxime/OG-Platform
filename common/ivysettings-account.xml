<ivysettings>
  <properties file="credentials.properties" />
  <credentials host="maven.opengamma.com" username="${opengamma.username}" passwd="${opengamma.password}" realm="Sonatype Nexus Repository Manager"/>
  <resolvers>
    <chain name="account" returnFirst="true">
        <ibiblio name="opengamna-account" m2compatible="true" root="http://maven.opengamma.com/nexus/content/groups/${opengamma.username}"/>
    </chain>
  </resolvers>
</ivysettings>
